{
  "root": true,
  "env": {
    "node": true,
    "jest": true,
    "browser": true,
    "jasmine": true
  },
  "extends": [
    "@node-sitecore/eslint-config/vue-cli"
  ],
  "globals": {
    "stub": true,
    "stubThrow": true
  },
  "rules": {
    "valid-jsdoc": 0,
    "comma-dangle": 0,
    "class-methods-use-this": "warn",
    "import/first": 0,
    "import/export": 0,
    "import/no-named-as-default-member": 0,
    "import/no-named-as-default": 0,
    "import/prefer-default-export": 0,
    "import/no-extraneous-dependencies": 0,
    "import/extensions": 0,
    "max-len": 0,
    "no-param-reassign": 0,
    "no-named-as-default": 0,
    "no-restricted-syntax": 0,
    "no-console": "error",
    "no-debugger": "error",
    "linebreak-style": 0
  },

  "parserOptions": {
    "parser": "babel-eslint"
  }
}
